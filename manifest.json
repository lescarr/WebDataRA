{
   "author": "@lescarr",
   "background": {
      "persistent": true,
      "scripts": [ "popup.js" ]
   },
   "browser_action": {
      "default_icon": "logo.png",
      "default_popup": "popup.html"
   },
   "description": "A Chrome extension that pastes data from Twitter, Google and FaceBook search results into an Excel spreadsheet.",
   "key": "",
   "manifest_version": 2,
   "name": "Web Data Research Assistant",
   "permissions": [ "tabs", "https://core.ac.uk/*", "https://github.com/search*", "https://scholar.google.co.uk/*", "https://scholar.google.com/*", "https://papers.ssrn.com/*", "https://www.facebook.com/", "https://www.google.com/search*", "https://www.google.co.uk/search*", "http://twitter.com/*", "https://twitter.com/*" ],
   "short_name": "WDRA",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.70"
}
